<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="DEBUG">
    <Appenders>
        <Console name="console">
            <PatternLayout pattern="%highlight{%d [%t] %-5p %c - %m%n}" />
        </Console>
    </Appenders>

    <Loggers>
        <Root level="debug">
<!--        <Root level="info">-->
            <AppenderRef ref="console" />
        </Root>
    </Loggers>

<!--    <Logger name="org.apache.hc.client5.http" level="debug"/>-->

</Configuration>
